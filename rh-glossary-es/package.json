{
  "name": "rh-glossary-es",
  "version": "1.0.0",
  "description": "Translation to Spanish for package rh-glossary.",
  "main": "rh-glossary-es.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "rf-locale --update-translations-file ../rh-glossary/**/*.js **/*.js --translations-filename ./translations_es.json --language es",
    "extract": "rf-locale --extract-translations-file ../rh-glossary/**/*.js **/*.js --translations-filename ./translations_es.json --language es",
    "validate": "rf-locale --validate-translations-file --translations-filename ./translations_es.json",
    "show": "rf-locale --show-translations-file --translations-filename ./translations_es.json"
  },
